<%- include("header") -%>
    <div class="box" id="heading">
        <h1> <%= DAY %> </h1>
    </div>
    
    <div class="box">
        <% for (var i = 0; i < LIST_ITEMS.length; i++) { %>
            <div class="item">
                <input type="checkbox">
                <p> <%= LIST_ITEMS[i] %> </p>
            </div>
        <% } %>
        

    <form class="item" action="/" method="POST">

        <input type="text" name="newItem" placeholder="New Item" autocomplete="off">
        
        <button type="submit" name="list" value= <%= %>>+</button>
    
    
    </form>

    </div>

<%- include("footer") -%> 